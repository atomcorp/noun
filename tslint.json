{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "interface-name": false,
    "jsx-boolean-value": false,
    "member-access": false,
    "no-implicit-dependencies": [true, ["enzyme", "enzyme-adapter-react-16"]]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["**/node_modules/**"]
  }
}
